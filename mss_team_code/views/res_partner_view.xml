<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <!--   Inherit form view     -->
        <record id="inherit_partner_sr_team_code_extension" model="ir.ui.view">
            <field name="name">Inherit Partner SR Team Code Extension</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='vat']" position="after">
                    <field name="team_code_id" string="Team Codes" />
                </xpath>

            </field>
        </record>

       
    </data>
</odoo>